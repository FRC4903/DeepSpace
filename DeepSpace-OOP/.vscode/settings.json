{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    ".classpath": true,
    ".project": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "wpilib.online": true,
  "files.associations": {
    "bitset": "cpp",
    "set": "cpp",
    "ostream": "cpp",
    "iostream": "cpp"
  },
  "C_Cpp.errorSquiggles": "Enabled"
}
